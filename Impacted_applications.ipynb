        "def detect_changed_business_values(dataframe):\n",
        "    changed_values = []\n",
        "    for _, row in dataframe.iterrows():\n",
        "        if row[\"Prior Version\"] != row[\"Current Version\"]:\n",
        "            changed_values.append(row[\"Business Value\"])\n",
        "    return changed_values"
      ],
      "metadata": {
